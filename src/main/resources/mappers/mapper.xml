<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
"-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="calendarMapper">
	
	<select id="selectBoardList" resultType="com.gn.spring.board.model.vo.Board" parameterType="com.gn.spring.board.model.vo.Board">
		SELECT *
		FROM `board` b
		JOIN `user` u ON b.board_writer = u.user_no
	</select>
	
</mapper>